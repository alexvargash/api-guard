{
    "name": "alexvargash/api-guard",
    "description": "A simple way of authenticating your APIs with API keys using Laravel",
    "keywords": [
        "api",
        "rest",
        "json",
        "api keys",
        "api authentication",
        "laravel"
    ],
    "homepage": "https://github.com/alexvargash/api-guard",
    "authors": [
        {
            "name": "Alex Vargas",
            "email": "alexvargas.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "*",
        "illuminate/database": "*",
        "ellipsesynergie/api-response": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Chrisbjr\\ApiGuard\\": "src"
        }
    },
    "minimum-stability": "stable"
}
